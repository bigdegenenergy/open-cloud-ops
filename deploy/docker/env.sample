# Docker Compose environment variables for local development.
# Copy this file to .env and fill in the values:
#   cp env.sample .env

# PostgreSQL
POSTGRES_DB=opencloudops
POSTGRES_USER=oco_user
POSTGRES_PASSWORD=changeme

# Cerebra Management API (leave empty to disable auth in development)
CEREBRA_ADMIN_API_KEY=

# Budget enforcement: "true" = allow requests when Redis unavailable
CEREBRA_BUDGET_FAIL_OPEN=true

# LLM Provider API Keys (optional, can also be passed per-request)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
